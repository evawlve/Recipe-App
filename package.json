{
  "name": "recipe-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint .",
    "lint:ci": "eslint . --max-warnings 500",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed:foods": "node scripts/seed-foods.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:seed": "prisma db seed",
    "usda:import": "ts-node --transpile-only scripts/usda-import.ts",
    "seed:curated": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/seed-curated.ts",
    "seed:curated:dry": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/seed-curated.ts --dry-run",
    "pack:fromcsv": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/curated-from-csv.ts",
    "seed:curated:batch": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/seed-curated-batch.ts",
    "lint:pack": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/lint-curated-pack.ts",
    "usda:saturate": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/usda-saturate.ts",
    "usda:saturate:mini": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/usda-saturate.ts --files=./data/usda/foundation-mini.json,./data/usda/sr-legacy-mini.json",
    "usda:saturate:both": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/usda-saturate.ts --files=./data/usda/fdc.json",
    "aliases:backfill": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/aliases-backfill.ts",
    "aliases:backfill:fast": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/aliases-backfill-fast.ts",
    "seed:tags": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register prisma/seeds/tags.seed.ts",
    "features:backfill": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/backfill-recipe-features.ts",
    "rollup:interactions": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/rollup-interactions.ts",
    "migrate:smoke": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/migrate-smoke.ts",
    "parser:bench": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/parser-bench.ts",
    "eval": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register eval/run.ts",
    "similar:build": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/build-similar.ts",
    "test:fdc-api": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/test-fdc-api.ts",
    "verify:seeding": "ts-node --project tsconfig.scripts.json --transpile-only -r tsconfig-paths/register scripts/verify-seeding.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.673.0",
    "@aws-sdk/s3-presigned-post": "3.673.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "5.18.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/papaparse": "^5.3.16",
    "@types/zxcvbn": "^4.4.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.544.0",
    "nanoid": "^5.1.6",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.64.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.0",
    "@types/react": "19.1.15",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "danger": "^13.0.5",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.0.0",
    "fast-check": "^4.3.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prisma": "5.18.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
